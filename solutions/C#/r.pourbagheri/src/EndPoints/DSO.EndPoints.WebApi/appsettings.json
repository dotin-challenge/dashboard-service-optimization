{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CacheSettings": {
    "AbsoluteExpirationMinutes": 60,
    "SlidingExpirationMinutes": 30,
    "CacheStampedeStrategy": "Semaphore",
    "SizeLimit": 1024
  }
}
